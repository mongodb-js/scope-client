{
  "name": "scout-client",
  "description": "A client for scout-server that works in the browser or a server.",
  "version": "0.4.0",
  "author": "Lucas Hrabovsky <lucas@mongodb.com> (http://imlucas.com)",
  "homepage": "http://github.com/10gen/scout",
  "repository": {
    "type": "git",
    "url": "git://github.com/10gen/scout.git"
  },
  "dependency-check": {
    "ignore": [
      "mongodb-datasets"
    ]
  },
  "scripts": {
    "check": "mongodb-js-precommit",
    "fmt": "mongodb-js-fmt lib/*.js test/*.js",
    "docs": "gulp docs",
    "start": "zuul --local 3001 --open -- test/*.test.js",
    "ci": "npm test",
    "test": "zuul --electron -- test/*.test.js",
    "prestart": "node bin/start-server.js",
    "poststart": "node bin/stop-server.js",
    "pretest": "node bin/start-server.js",
    "posttest": "node bin/stop-server.js"
  },
  "precommit": [
    "check"
  ],
  "dependencies": {
    "ampersand-model": "^6.0.2",
    "async": "^1.5.0",
    "debug": "^2.2.0",
    "event-stream": "^3.3.2",
    "lodash.omit": "^3.1.0",
    "lodash.trimright": "^3.0.2",
    "mongodb-connection-model": "^3.0.5",
    "mongodb-extended-json": "^1.5.3",
    "mongodb-ns": "^1.0.0",
    "raf": "^3.1.0",
    "socket.io-client": "^1.3.7",
    "socket.io-stream": "0.9.0",
    "superagent": "^1.4.0"
  },
  "devDependencies": {
    "backbone": "^1.2.3",
    "bson": "^0.4.19",
    "del": "^2.0.2",
    "dox": "^0.8.0",
    "electron-prebuilt": "0.34.1",
    "eslint-config-mongodb-js": "^1.0.4",
    "gulp": "^3.9.0",
    "gulp-util": "^3.0.7",
    "handlebars": "^4.0.3",
    "lodash.range": "^3.0.1",
    "mocha": "^2.3.3",
    "mongodb-datasets": "^0.1.5",
    "mongodb-js-fmt": "^0.0.3",
    "mongodb-js-precommit": "^0.2.2",
    "mongodb-runner": "^3.1.12",
    "pre-commit": "^1.1.2",
    "scout-server": "http://bin.mongodb.org/js/scout-server/v0.4.0/scout-server-0.4.0.tar.gz",
    "through2": "^2.0.0",
    "zuul": "^3.7.2"
  }
}
